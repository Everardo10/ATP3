 
public class Circulo implements Forma{
    private double r;
  public Circulo (double r){
      this.r=r;
  }
  public Circulo (){
  }
  public double getRaio(){
      return r;
  }
   public double area() {
    return Math.PI * Math.pow(getRaio(), 2.0);
  }
  public double perimetro() {
    return 2.0 * Math.PI * getRaio();
  }
}
